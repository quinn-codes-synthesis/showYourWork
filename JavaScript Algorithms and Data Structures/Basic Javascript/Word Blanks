function wordBlanks(myNoun, myAdjective, myVerb, myAdverb) {
  // Your code below this line
  var result = "I've never seen a " + myNoun + " fetch such an enormous tree branch before. And she " + myVerb + " like she was holding a twig, not an entire limb twice her length and almost as " + myAdjective + " around. I " + myAdverb + " realized that her super-strength could net me millions of subscribers on MeTube, and started taking video.";

  // Your code above this line
  return result;
}

// Change the words here to test your function
wordBlanks("dog", "big", "ran", "quickly");
